@{
    ViewBag.Title = "FoDT Web Accounts Management";
    ViewBag.SubTitle = "Admin";
}

@section additionalHead {
<script type="text/javascript">

    fodt.getData = function (url, data, onSuccess, onError) {
        $.ajax({
            url: url,
            data: data,
            cache: false,
            success: onSuccess,
            error: onError
        });
    };

    $(function () {
        fodt.getData("@Url.Action(MVC.Accounts.ListAccounts())", null, function (data) {
            alert(data);
        }, function () { alert("error") });
    });
</script>
}